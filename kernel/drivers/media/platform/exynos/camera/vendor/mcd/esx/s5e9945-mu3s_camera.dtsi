#include <dt-bindings/camera/exynos_is_dt.h>

#include "module_hp2.dtsi"
#include "module_imx754.dtsi"
#include "module_imx754_mirrorflip.dtsi"
#include "module_imx564.dtsi"
#include "module_3lu.dtsi"

/ {
	fragment@camera {
		target-path = "/";
		__overlay__ {
			#address-cells = <2>;
			#size-cells = <1>;
			interrupt-parent = <&gic>;

			is_flash_gpio: is-flash-gpio@0 {
				compatible = "samsung,sensor-flash-s2mpb02";
				sensor-module = <&is_sensor_hp2>, <&is_sensor_imx754>, <&is_sensor_imx754_mirrorflip>;
				id = <0 2 3>;
				status = "okay";

				torch-gpio = <&gpc1 1 0x1>;
			};

			is_laser_af_vl53l8: is-laser-af-vl53l8@0 {
				compatible = "samsung,laser-af-vl53l8";
				id = <0 2 3 4>;
				status = "okay";
			};

			/* OIS_MCU */
			ois_mcu: ois_mcu@1A310000 {
				pinctrl-names = "default";
				pinctrl-0 = <&ois_spi0 &ois_spi1>;
				compatible = "samsung,sensor-ois-mcu";
				reg = <0x0 0x1A310000 0x18000>, /* OIS MCU (SRAM 96KB) */
					<0x0 0x119D0000 0x10000>, /* I2C: USI05_USI */
					<0x0 0x119E0000 0x10000>,	/* SPI: USI06_USI */
					<0x0 0x11830000 0x1000>,	/* PERI setting */
					<0x0 0x1A300000 0x10000>; /* SFR */
				reg-names = "core", "i2c", "spi", "peri", "sfr";
				interrupts = <0 INTREQ__OIS_MCU_CSIS IRQ_TYPE_LEVEL_HIGH>;
				id = <0 2 3>;
				status = "okay";
				mcu_ctrl_actuator = <0 0 1>;
				mcu_skip_video_fastae;
				mcu_off_during_uwonly_mode;
				ois_gyro_direction = <0x00 0x01 0x31 0x00 0x01 0x00 0x00>;

				power-domains = <&pd_csis>;
				clocks = <&clock GATE_OIS_MCU_TOP_QCH>, <&clock DOUT_DIV_CLK_PERIC2_USI06>;
				clock-names = "user_mux", "ipclk_spi";
			};

			/* REAR CAMERA */
			is_sensor0: is_sensor0@1A030000 {
				reg = <0x0 0x1A040000 0x1000>, /* MIPI-CSI1 */
					<0x0 0x1A0F0000 0x10000>, /* PHY: M0S4S4S4S4S4S2 */
					<0x0 0x1A19022C 0x10>, /* CSIS_FRO */
					<0x0 0x1A190254 0x10>; /* CSIS 7x5 OTF_OUT MUX */
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */

				id = <0>;
				csi_ch = <1>;
				status = "okay";
				use_cphy = <1>;

				interrupts = <0 INTREQ__CSIS1 IRQ_TYPE_LEVEL_HIGH>; /* MIPI-CSI1 */
				phys = <&mipi_phy_csis1_m0s4s4s4s4s4s4_s22 0>;
				phy_setfile = <&cphy_setfile>;
			};

			/* FRONT CAMERA */
			is_sensor1: is_sensor1@1A040000 {
				reg = <0x0 0x1A030000 0x1000>, /* MIPI-CSI0 */
					<0x0 0x1A0F0000 0x10000>, /* PHY: M0S4S4S4S4S4S2 */
					<0x0 0x1A19022C 0x10>, /* CSIS_FRO */
					<0x0 0x1A190254 0x10>; /* CSIS 7x5 OTF_OUT MUX */
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <1>;
				csi_ch = <0>;
				status = "okay";
				use_cphy = <1>;
				scramble = <1>; /* need to sync with sensor setting */

				interrupts = <0 INTREQ__CSIS0 IRQ_TYPE_LEVEL_HIGH>; /* MIPI-CSI0 */
				phys = <&mipi_phy_csis0_m0s4s4s4s4s4s4_s22 0>;
				phy_setfile = <&cphy_setfile>;
			};

			/* Tele x3 CAMERA */
			is_sensor2: is_sensor2@1A050000 {
				reg = <0x0 0x1A050000 0x1000>, /* MIPI-CSI2 */
					<0x0 0x1A0F0000 0x10000>, /* PHY: M0S4S4S4S4S4S2 */
					<0x0 0x1A19022C 0x10>, /* CSIS_FRO */
					<0x0 0x1A190254 0x10>; /* CSIS 7x5 OTF_OUT MUX */
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <2>;
				csi_ch = <2>;
				status = "okay";
				use_cphy = <1>;

				interrupts = <0 INTREQ__CSIS2 IRQ_TYPE_LEVEL_HIGH>; /* MIPI-CSI2 */
				phys = <&mipi_phy_csis2_m0s4s4s4s4s4s4_s22 0>;
				phy_setfile = <&cphy_setfile>;
			};

			/* Tele x10 CAMERA */
			is_sensor3: is_sensor3@1A060000 {
				reg = <0x0 0x1A070000 0x1000>, /* MIPI-CSI4 */
					<0x0 0x1A0F0000 0x10000>, /* PHY: M0S4S4S4S4S4S2 */
					<0x0 0x1A19022C 0x10>, /* CSIS_FRO */
					<0x0 0x1A190254 0x10>; /* CSIS 7x5 OTF_OUT MUX */
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <3>;
				csi_ch = <4>;
				status = "okay";
				use_cphy = <1>;

				interrupts = <0 INTREQ__CSIS4 IRQ_TYPE_LEVEL_HIGH>; /* MIPI-CSI4 */
				phys = <&mipi_phy_csis4_m0s4s4s4s4s4s4_s22 0>;
				phy_setfile = <&cphy_setfile>;
			};

			/* ULTRA WIDE CAMERA */
			is_sensor4: is_sensor4@1A070000 {
				reg = <0x0 0x1A060000 0x1000>, /* MIPI-CSI3 */
					<0x0 0x1A0F0000 0x10000>, /* PHY: M0S4S4S4S4S4S2 */
					<0x0 0x1A19022C 0x10>, /* CSIS_FRO */
					<0x0 0x1A190254 0x10>; /* CSIS 7x5 OTF_OUT MUX */
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <4>;
				csi_ch = <3>;
				status = "okay";
				use_cphy = <1>;

				interrupts = <0 INTREQ__CSIS3 IRQ_TYPE_LEVEL_HIGH>; /* MIPI-CSI3 */
				phys = <&mipi_phy_csis3_m0s4s4s4s4s4s4_s22 0>;
				phy_setfile = <&cphy_setfile>;
			};

			is_sensor5: is_sensor5@1A080000 {
				scenario = <SENSOR_SCENARIO_NORMAL>;    /* Normal, Vision, OIS etc */
				id = <1>;
				csi_ch = <1>;
				status = "disabled";
			};

			is_sensor_3lu: is_sensor_3lu@10 {
				power_seq {
					scenario_normal_on {
						7 { pname; pin; share; };
						10 { pname = "VDD_CAMIO_1P8"; pin = <PIN_REGULATOR 1 1>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
						40 { pname = "VDD_VTCAM_AF_2P8"; pin = <PIN_OUTPUT 1 0>; gpio = <&gpb1 0 0x1>; actuator_i2c_delay = <10000>; };
					};

					scenario_normal_off {
						70 { pname = "VDD_VTCAM_AF_2P8"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpb1 0 0x1>; };
						100 { pname = "VDD_CAMIO_1P8"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN0 0>; };
						120 { pname; pin; share; };
					};

					scenario_vision_on {
						7 { pname; pin; share; };
						10 { pname = "VDD_CAMIO_1P8"; pin = <PIN_REGULATOR 1 1>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
						40 { pname = "VDD_VTCAM_AF_2P8"; pin = <PIN_OUTPUT 1 0>; gpio = <&gpb1 0 0x1>; actuator_i2c_delay = <10000>; };
					};

					scenario_vision_off {
						70 { pname = "VDD_VTCAM_AF_2P8"; pin = <PIN_OUTPUT 0 0>; gpio = <&gpb1 0 0x1>; };
						100 { pname = "VDD_CAMIO_1P8"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN0 0>; };
						120 { pname; pin; share; };
					};

					scenario_read_rom_on {
						10 { pname = "VDD_CAMIO_1P8"; pin = <PIN_REGULATOR 1 5000>; share = <SRT_ACQUIRE SHARED_PIN0 1>; };
					};

					scenario_read_rom_off {
						20 { pname = "VDD_CAMIO_1P8"; pin = <PIN_REGULATOR 0 0>; share = <SRT_RELEASE SHARED_PIN0 0>; };
					};
				};
			};
		}; /* end of __overlay__ */
	}; /* end of fragment */
}; /* end of root */

/***** reference symbol node *****/
&pinctrl_1 {
	/* ULTRA WIDE */
	hsi2c37_bus_in: hsi2c37-bus-in {
		samsung,pins = "gpm2-0", "gpm2-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* Tele 3x AF */
	hsi2c38_bus_in: hsi2c38-bus-in {
		samsung,pins = "gpm3-0", "gpm3-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* FRONT */
	hsi2c43_bus_in: hsi2c43-bus-in {
		samsung,pins = "gpm8-0", "gpm8-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};
};

&pinctrl_6 {
