	/* FRONT AF */
	hsi2c14_bus_in: hsi2c14-bus-in {
		samsung,pins = "gpp7-0", "gpp7-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* REAR WIDE AF */
	hsi2c15_bus_in: hsi2c15-bus-in {
		samsung,pins = "gpp7-2", "gpp7-3";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* Tele 10x */
	hsi2c17_bus_in: hsi2c16-bus-in {
		samsung,pins = "gpp8-2", "gpp8-3";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};
};

&pinctrl_7 {
	/* REAR WIDE */
	hsi2c33_bus_in: hsi2c33-bus-in {
		samsung,pins = "gpc9-0", "gpc9-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* UW AF EEPROM */
	hsi2c11_bus_in: hsi2c11-bus-in {
		samsung,pins = "gpp5-2", "gpp5-3";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-drv = <0>;
	};

	/* ois spi 0*/
	ois_spi0: ois-spi0 {
		samsung,pins = "gpp6-0", "gpp6-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <1>;
	};

	/* ois spi 1*/
	ois_spi1: ois-spi1 {
		samsung,pins = "gpp6-2", "gpp6-3";
		samsung,pin-function = <0>;
		samsung,pin-pud = <0>;
		samsung,pin-con-pdn = <2>;
		samsung,pin-pud-pdn = <0>;
	};
};

&hsi2c_33 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpc9 0 0 &gpc9 1 0>;
	status = "okay";
	clock-frequency = <1000000>;
	samsung,reset-before-trans;
	samsung,fast-plus-mode;

	pinctrl-names = "default","on_i2c","off_i2c";
	pinctrl-0 = <&hsi2c33_bus_in>;
	pinctrl-1 = <&hsi2c33_bus>;
	pinctrl-2 = <&hsi2c33_bus_in>;

	is-hp2@10 {
		compatible = "samsung,exynos-is-cis-hp2";
		sensor-module = <&is_sensor_hp2>;
		reg = <0x10>; /* 1 bit right shift */
		id = <0>; /* matching is_sensor id */
		setfile = "setA";
		rev_reg = <0x0002 2 0xB100>;
		vendor_use_adaptive_mipi;
	};

	rear_eeprom@50 {
		compatible = "samsung,is-device-rom-i2c";
		reg = <0x50>;

		rom_id = <0>; /* ROM_ID_REAR */
		rom_type = <2>; /* ROM_TYPE_EEPROM */
		rom_power_position = <0>;
		rom_size = <0x8000>;
		cal_map_es_version = <31>;
		camera_module_es_version = "A";
		ignore_cal_crc_error;

		/* Diamond_project_DM3_Rear_Cal_Map_V031_20220713_for_all */
		/* 0 header */
		header_crc_check_list = <0x0000 0x01FB 0x01FC>;
		/* 0 LSI Wide LSC data */
		/* 1 Wide camera */
		/* 2 ATC data */
		/* 3 Tele camera */
		/* 4 Sensor */
		crc_check_list = <
			0x0200 0x15FB 0x15FC
			0x1D00 0x2D2B 0x2D2C
			0x2D30 0x3D5B 0x3D5C
			0x3D60 0x3D8B 0x3D8C
			0x3D90 0x3DCB 0x3DCC
			0x3DD0 0x3E3B 0x3E3C
			0x3E40 0x3E9B 0x3E9C
			0x3EA0 0x3ECB 0x3ECC
			0x1D00 0x3EDF 0x3EFC
			0x3F00 0x3FFB 0x3FFC
			0x4000 0x5C1F 0x5C20
			0x5C24 0x658D 0x658E
			0x6592 0x69B7 0x69B8
			0x69BC 0x7349 0x734A
			0x734E 0x7AA5 0x7AA6
			0x4000 0x7ADF 0x7AFC
			0x7B00 0x7C4B 0x7C4C
		>;

		/* Master */
		rom_header_version_start_addr = <0x50>;
		rom_header_cal_data_start_addr = <0x200>;
		rom_header_cal_map_ver_start_addr = <0x70>;
		rom_header_isp_setfile_ver_start_addr = <0x74>;
		rom_header_project_name_start_addr = <0x7E>;
		rom_header_module_id_addr = <0xAE>; /* 0xA8 + reserved bytes */
		rom_header_sensor_id_addr = <0xB8>;
		rom_header_mtf_data_addr = <0xE0>;
		rom_paf_cal_start_addr = <0x1D00>;
		rom_awb_master_addr = <0x3D90>;
		rom_awb_module_addr = <0x3DB4>;
		/* Near Far Mid1 ... Mid7 */
		rom_af_cal_addr = <0x3D70 0x3D64 0x3D68 0x3D6C>;

		/* index, start, end, endian[little:0x00, big:0x01, check:0x02, continue:0x03]  */
		rom_xtc_cal_data_addr_list = <
			0x00 0x4000 0x56B7 0x02 /* HEXA_HQ */
			0x01 0x6592 0x6709 0x02 /* IMBACC */
			0x02 0x734E 0x764F 0x02 /* OFFSETS_GAIN for Hexa */
			0x03 0x7650 0x770F 0x03 /* OFFSETS_GAIN for Tetra */
			0x04 0x7710 0x774F 0x03 /* OFFSETS_GAIN for IMB */
			0x05 0x69BC 0x708B 0x02 /* SPECIAL */
			0x06 0x5C24 0x60DB 0x02 /* TETRA_HQ */
		>;

 		/* sensor ois - xgg ygg xcoef ycoef xsupper ysupper calmark */
		/* sensor2 ois - xgg ygg xcoef ycoef xsupper ysupper calmark */
		/* TO DO */
		rom_ois_list = <0x2E20 0x2E24 0x2E2C 0x2E2E 0x2E48 0x2E4A 0x2E40
			0x3630 0x3634 0x363C 0x363E 0x3658 0x365A 0x3650>;
	};
};

&hsi2c_15 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpp7 2 0 &gpp7 3 0>;
	status = "okay";
	clock-frequency = <1000000>;
	samsung,reset-before-trans;
	samsung,fast-plus-mode;

	pinctrl-names = "default","on_i2c","off_i2c";
	pinctrl-0 = <&hsi2c15_bus_in>;
	pinctrl-1 = <&hsi2c15_bus>;
	pinctrl-2 = <&hsi2c15_bus_in>;

	is-actuator@0C { /* wide AF */
		compatible = "samsung,exynos-is-actuator-ak737x";
		sensor-module = <&is_sensor_hp2>;
		reg = <0x0C>;
		id = <0>; /* matching is_sensor id */
		/* AK7314 : addr 0xF3, data 0x19 */
		vendor_product_id = <0xF3 0x13 0xF3 0x19>;
		vendor_first_pos = <2000>;  /* 12bits */
		vendor_first_delay = <2000>;
		vendor_soft_landing_seqid = <3>;
		vendor_soft_landing_list = <640 30 64 30>; /* 1st_pos delay 2nd_pos delay */
		vendor_use_sleep_mode;
		vendor_use_standby_mode; /* AK7314 with vendor_use_sleep_mode */
		/* delay settings for AK7314 */
		vendor_sleep_to_standby_delay = <2200>;
		vendor_active_to_standby_delay = <300>;
	};

	rear4_eeprom@54 { /* tele x10 */
		compatible = "samsung,is-device-rom-i2c";
		reg = <0x54>;

		rom_id = <6>; /* ROM_ID_REAR4 */
		rom_type = <2>; /* ROM_TYPE_EEPROM */
		rom_power_position = <6>;
		rom_size = <0x4000>;
		cal_map_es_version = <23>;
		camera_module_es_version = "A";

		/* Rainbow_project_B0_Rear_Cal_Map_V023_20210520_for_all */
		/* 0 header */
		header_crc_check_list = <0x0000 0x01FB 0x01FC>;
		/* 0 LSI Wide LSC data */
		/* 1 Tele camera */
		/* 2 ATC data */
		crc_check_list = <
			0x0200 0x15FB 0x15FC
			0x1D00 0x2D2B 0x2D2C
			0x2D30 0x2D5B 0x2D5C
			0x2D60 0x2D9B 0x2D9C
			0x2DA0 0x2E0B 0x2E0C
			0x2E10 0x361B 0x361C
			0x3620 0x367B 0x367C
			0x1D00 0x36DF 0x36FC
			0x3700 0x37FB 0x37FC
		>;

		/* Master */
		rom_header_version_start_addr = <0x40>;
		rom_header_cal_data_start_addr = <0x200>;
		rom_header_cal_map_ver_start_addr = <0x70>;
		rom_header_isp_setfile_ver_start_addr = <0x74>;
		rom_header_project_name_start_addr = <0x7E>;
		rom_header_module_id_addr = <0xAE>; /* 0xA8 + reserved bytes */
		rom_header_sensor_id_addr = <0xC8>;
		rom_header_mtf_data_addr = <0x182>;
		rom_awb_master_addr = <0x2D60>;
		rom_awb_module_addr = <0x2D84>;
		/* Near Far Mid1 ... Mid7 */
		rom_af_cal_addr = <0x2D40 0x2D34 0x2D38 0x2D3C>;

		rom_dualcal_slave0_start_addr = <0x2E10>;
		rom_dualcal_slave0_size = <0x080C>; /* 2060 */
		/* x y z sx sy */
		/* range max_err avg_err dll_version prj_name */
		rom_dualcal_slave0_tilt_list = <0x2EC8 0x2ECC 0x2ED0 0x2EEC 0x2EF0
			0x30E2 0x30E6 0x30EA 0x3104 0x30EF>;

		/* sensor ois - xgg ygg xcoef ycoef xsupper ysupper calmark */
		/* sensor2 ois - xgg ygg xcoef ycoef xsupper ysupper calmark */
		/* TO DO */
		rom_ois_list = <0x2E20 0x2E24 0x2E2C 0x2E2E 0x2E48 0x2E4A 0x2E40
			0x3630 0x3634 0x363C 0x363E 0x3658 0x365A 0x3650>;
	};
};

