&hsi2c_14 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpp7 0 0 &gpp7 1 0>;
	status = "okay";
	clock-frequency = <1000000>;
	samsung,reset-before-trans;
	samsung,fast-plus-mode;

	pinctrl-names = "default","on_i2c","off_i2c";
	pinctrl-0 = <&hsi2c14_bus_in>;
	pinctrl-1 = <&hsi2c14_bus>;
	pinctrl-2 = <&hsi2c14_bus_in>;

	is-actuator@0C { /* front AF */
		compatible = "samsung,exynos-is-actuator-ak737x";
		sensor-module = <&is_sensor_3lu>;
		reg = <0x0C>;
		id = <1>; /* matching is_sensor id */
		/* DW9825A */
		vendor_product_id = <0xF3 0x0E>;
		vendor_first_pos = <960>;  /* 12bits */
		vendor_first_delay = <2000>;
		vendor_use_sleep_mode;
		vendor_use_standby_mode; /* with vendor_use_sleep_mode */
		vendor_use_hall;
		vendor_hall_min_interval = <75>; /* ms */
		vendor_hall_hw_param_tolerance = <400>;
		/* delay settings for DW9825A */
		vendor_sleep_to_standby_delay = <3000>;
		vendor_active_to_standby_delay = <300>;
	};
};

&hsi2c_37 {
	#address-cells = <1>;
	#size-cells = <0>;
	gpios = <&gpm2 0 0 &gpm2 1 0>;
	status = "okay";
	clock-frequency = <1000000>;
	samsung,reset-before-trans;
	samsung,fast-plus-mode;

	pinctrl-names = "default","on_i2c","off_i2c";
	pinctrl-0 = <&hsi2c37_bus_in>;
	pinctrl-1 = <&hsi2c37_bus>;
	pinctrl-2 = <&hsi2c37_bus_in>;

	is-imx564@10 { /* ultrawide */
		compatible = "samsung,exynos-is-cis-imx564";
		sensor-module = <&is_sensor_imx564>;
		reg = <0x10>; /* 1 bit right shift */
		id = <4>; /* matching is_sensor id */
		setfile = "setB";
		status = "okay";
		rev_reg = <0x36F2 2 0x564C>;
		vendor_use_adaptive_mipi;
	};

	rear2_eeprom@51 { /* Ultra wide eeprom */
		compatible = "samsung,is-device-rom-i2c";
		reg = <0x51>;

		rom_id = <2>; /* ROM_ID_REAR2 */
		rom_type = <2>; /* ROM_TYPE_EEPROM */
		rom_power_position = <4>;
		rom_size = <0x2000>;
		cal_map_es_version = <31>;
		camera_module_es_version = "A";

		/* Muse_project_Muse12_Rear_Cal_Map_V031_20230525_for_all */
		/* 0 header */
		header_crc_check_list = <0x0000 0x00FB 0x00FC>;
		/* 0 master LSC */
		/* 1 master all */
		crc_check_list = <
			0x0100 0x14FB 0x14FC
			0x1500 0x1BFB 0x1BFC
			0x1C00 0x1C3B 0x1C3C
			0x1C40 0x1CAB 0x1CAC
			0x1C00 0x1CDF 0x1CFC
			0x1D00 0x1E4B 0x1E4C
		>;
		/* Master */
		rom_header_version_start_addr = <0x40>;
		rom_header_cal_data_start_addr = <0x100>;
		rom_header_cal_map_ver_start_addr = <0x50>;
		rom_header_isp_setfile_ver_start_addr = <0x54>;
		rom_header_project_name_start_addr = <0x5E>;
		rom_header_module_id_addr = <0xAE>; /* 0xA8 + reserved bytes */
		rom_header_sensor_id_addr = <0xB8>;
		rom_header_mtf_data_addr = <0x66>; /* AF position */
		rom_awb_master_addr = <0x1C00>;
		rom_awb_module_addr = <0x1C24>;
	};
};

&hsi2c_36 {
	s2mpb02_pmic@59 {
		torch {
			torch1-gpio = <&gpc1 1 0x1>;

			status = "okay";

			leds1 {
				ledname = "leds-sec1";
				id = <0>;
				brightness = <0xE>; /* adaptive Nx100+50 : 1450mA */
				timeout = <0x7>; /* (N+1)x62.5 : 500ms */
				torch_table_enable = <0>;
			};

			leds2 {
				ledname = "torch-sec1";
				id = <1>;
				brightness = <0xE>; /* Nx20 = 280mA */
				timeout = <0xF>;
				torch_table_enable = <1>;
				torch_table = <3 5 5 6 6 8 8 8 10 10>;
				/* 60mA 100mA 120mA 160mA 200mA */
			};
		};
	};
};

