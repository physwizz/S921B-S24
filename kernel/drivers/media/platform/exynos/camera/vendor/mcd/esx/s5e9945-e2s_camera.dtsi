	/* ois spi 0*/
	ois_spi0: ois-spi0 {
		samsung,pins = "gpp6-0", "gpp6-1";
		samsung,pin-function = <0>;
		samsung,pin-pud = <1>;
	};

	/* ois spi 1*/
	ois_spi1: ois-spi1 {
		samsung,pins = "gpp6-2", "gpp6-3";
