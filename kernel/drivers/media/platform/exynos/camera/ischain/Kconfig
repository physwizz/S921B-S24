config PABLO_V10_1_0
	bool "Use PABLO v10.1.0"
	depends on SOC_S5E9925
	depends on !SOC_S5E9925_EVT0
	default y
	select EXYNOS_DEVICE_MIPI_CSIS_VER4
	select CSIS_V8_0
	select PABLO_CAMIF_BNS
	select CAMERA_CIS_SELECT
	select CAMERA_ACT_SELECT
	select CAMERA_FLASH_SELECT
	select CAMERA_PDP
	select CAMERA_DVFS_V2
	select CAMERA_MEM_V2
	select PDP_V4_1
	select CSTAT_V1_1
	select BYRP_V1_1
	select RGBP_V1_1
	select MCFP_V10_1
	select YUVP_V1_1
	select LME_V2_1
	select COMMON_DMA_V1_0
	select PABLO_HW_HELPER_V1
	help
	  PABLO ISP version 10.1.0

