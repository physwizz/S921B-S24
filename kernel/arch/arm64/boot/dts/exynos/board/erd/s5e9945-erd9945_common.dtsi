			eusb_repeater_tune:hs_tune_repeater {
				status = "disabled";
				repeater_tune_cnt = <5>;
				/* 0xffffffff means using default value */
				/* tune value - reg value shift mask*/
				repeater_tune1 {
					tune_name = "tx_amp";
					tune_value = <0x08 0x3 0x0 0x7>;
				};

				repeater_tune2 {
					tune_name = "tx_de_emp";
					tune_value = <0x07 0x3 0x0 0x7>;
				};

				repeater_tune3 {
					tune_name = "tx_de_width";
